<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gender_form_background"
    tools:context="com.example.android.bmi_app.CalorieslDataActivity">


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/guideline78"
        app:layout_constraintLeft_toLeftOf="@+id/guideline84"
        app:layout_constraintTop_toTopOf="@+id/guideline130"
        app:layout_constraintVertical_bias="0.333">


        <TextView

            android:id="@+id/yLabel"
            android:layout_marginLeft="38dp"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2017"
            android:textColor="#002aff"
            android:textSize="12sp"/>

        <TextView
            android:id="@+id/mLabel"
            android:layout_gravity="center"
            android:layout_width="100dp"
            android:layout_height="20dp"
            android:background="#000000"
            android:gravity="center"
            android:text="Sep"
            android:textColor="@color/White"/>

        <TextView
            android:id="@+id/dLabel"
            android:layout_gravity="center"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@color/White"
            android:gravity="center"
            android:text="3"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/eLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Sun"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_marginLeft="38dp"
            android:layout_marginTop="-25dp"/>
    </LinearLayout>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline78"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.28"
        tools:layout_editor_absoluteY="159dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline84"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.68" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline130"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.03"
        tools:layout_editor_absoluteY="17dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline79"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.64" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Burn calories list"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="@color/background"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline136"
        app:layout_constraintBottom_toTopOf="@+id/guideline79"
        app:layout_constraintRight_toLeftOf="@+id/guideline137"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/guideline142"
        android:layout_marginTop="8dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline136"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.01"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="4dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline137"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Earn calories list"
        android:textSize="16sp"
        android:textColor="@color/background"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/guideline137"
        app:layout_constraintBottom_toTopOf="@+id/guideline79"
        app:layout_constraintTop_toTopOf="@+id/guideline142"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:text="Calories"
        android:textColor="@color/background"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline139"
        app:layout_constraintEnd_toStartOf="@+id/guideline137"
        app:layout_constraintLeft_toLeftOf="@+id/guideline136"
        app:layout_constraintTop_toTopOf="@+id/guideline130" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline138"
        app:layout_constraintGuide_begin="528dp"
        android:orientation="horizontal" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline139"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="60dp" />

    <Button
        android:id="@+id/Earn_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/plus_list"
        app:layout_constraintRight_toLeftOf="@+id/guideline137"
        android:layout_marginRight="24dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline136"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline145"
        app:layout_constraintTop_toTopOf="@+id/guideline79"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Burn_list_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/calorie_list"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline136"
        app:layout_constraintRight_toLeftOf="@+id/guideline137"
        android:layout_marginRight="24dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline142"
        app:layout_constraintTop_toTopOf="@+id/guideline143"
        android:layout_marginTop="16dp"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Earn_list_calories"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/calorie_list"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline137"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline142"
        app:layout_constraintTop_toTopOf="@+id/guideline143"
        android:layout_marginTop="16dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/burn_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/plus_list"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline137"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline145"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline142"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.57" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline143"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline145"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="63dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Earn calorie Total"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/background"
        app:layout_constraintRight_toLeftOf="@+id/guideline137"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline136"
        app:layout_constraintTop_toTopOf="@+id/guideline145"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline138" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Burn calorie Total"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/background"
        app:layout_constraintLeft_toLeftOf="@+id/guideline137"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline145"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline138" />

    <com.sunfusheng.marqueeview.MarqueeView
        android:id="@+id/marqueeView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/holo_blue_bright"
        app:mvAnimDuration ="500"
        app:mvTextColor ="@android:color/black"
        app:mvSingleLine ="true"
        app:mvTextSize = "12sp"
        app:mvInterval="1500"
        app:mvGravity ="center"
        android:text="TextView"


        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline143"
        app:layout_constraintTop_toTopOf="@+id/guideline78"
        android:layout_marginTop="0dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/calorie_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/guideline78"
        app:layout_constraintEnd_toStartOf="@+id/guideline137"
        app:layout_constraintStart_toStartOf="@+id/guideline136"
        app:layout_constraintTop_toTopOf="@+id/guideline139"
        tools:ignore="MissingConstraints" />


</android.support.constraint.ConstraintLayout>
