<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/gender_form_background"
tools:context="com.example.android.bmi_app.WeightDataActivity">


<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline80"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.33"
    tools:layout_editor_absoluteY="187dp"
    tools:layout_editor_absoluteX="0dp" />

<TextView
    android:id="@+id/textView13"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:gravity="center"
    android:text="Statics"
    android:textStyle="bold"
    android:textColor="@color/background"
    android:textSize="20sp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toTopOf="@+id/guideline82"
    app:layout_constraintVertical_bias="1.0" />

<EditText
    android:id="@+id/inputTextY"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:ems="10"
    android:inputType="textPersonName"
    android:hint="Weight..."
    android:textColorHint="@color/background"
    app:layout_constraintTop_toTopOf="@+id/guideline82"
    android:layout_marginTop="8dp"
    app:layout_constraintRight_toLeftOf="@+id/guideline85"
    android:layout_marginRight="8dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@+id/guideline81"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintVertical_bias="0.0"
    android:layout_marginLeft="8dp"
    app:layout_constraintLeft_toLeftOf="@+id/guideline70" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline81"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.25"
    tools:layout_editor_absoluteY="142dp"
    tools:layout_editor_absoluteX="0dp" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline82"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.14" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline83"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.0" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline85"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.73"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="263dp" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline86"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.73"
    tools:layout_editor_absoluteY="414dp"
    tools:layout_editor_absoluteX="0dp" />

<Button
    android:id="@+id/insertButton"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/write"
    app:layout_constraintTop_toTopOf="@+id/guideline82"
    app:layout_constraintLeft_toLeftOf="@+id/guideline85"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintRight_toLeftOf="@+id/guideline88"
    app:layout_constraintBottom_toTopOf="@+id/guideline81"
    app:layout_constraintVertical_bias="0.0" />

<com.jjoe64.graphview.GraphView
    android:id="@+id/graph_view"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@color/White"
    app:layout_constraintBottom_toTopOf="@+id/guideline86"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginBottom="0dp"
    android:layout_marginRight="0dp"
    app:layout_constraintTop_toTopOf="@+id/guideline81"
    android:layout_marginTop="0dp"
    android:layout_marginLeft="0dp"
    app:layout_constraintLeft_toLeftOf="@+id/guideline83"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintVertical_bias="0.0" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline88"
    app:layout_constraintGuide_begin="333dp"
    android:orientation="vertical" />


<com.hookedonplay.decoviewlib.DecoView
    android:layout_width="185dp"
    android:layout_height="139dp"
    android:id="@+id/dynamicArcView"
    android:layout_marginBottom="7dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toTopOf="@+id/guideline86"
    android:layout_marginTop="8dp"
    app:layout_constraintVertical_bias="1.0"
    android:layout_marginLeft="32dp"
    app:layout_constraintLeft_toLeftOf="@+id/guideline83"
    app:layout_constraintRight_toLeftOf="@+id/guideline88"
    android:layout_marginRight="8dp" />

<TextView
    android:id="@+id/textPercentage"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="TextView"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="@+id/dynamicArcView"
    android:layout_marginTop="8dp"
    app:layout_constraintBottom_toBottomOf="@+id/dynamicArcView"
    android:layout_marginBottom="8dp" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline70"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.18" />

<TextView
    android:id="@+id/textView14"
    android:layout_width="70dp"
    android:layout_height="76dp"
    android:layout_marginLeft="16dp"
    android:text="Weight average"
    android:textColor="@color/background"
    android:textStyle="bold"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toTopOf="@+id/guideline86"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@+id/guideline72"
    app:layout_constraintRight_toLeftOf="@+id/guideline71"
    android:layout_marginRight="0dp"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintVertical_bias="0.028" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline71"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.25" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline72"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.83" />
</android.support.constraint.ConstraintLayout>
